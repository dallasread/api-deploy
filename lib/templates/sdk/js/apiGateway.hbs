var API = {
    debug: false,

{{#each paths}}
    {{#each .}}
        {{#with x-apiDeploy}}
            {{#if sdk.method}}
    {{sdk.method}}: function {{sdk.method}}(data, done) {
    {{#if lambda.arn}}
        $.ajax({
            method: '{{@key}}',
            url: '{{@../key}}',
            data: data,
            complete: done
        });
    {{else}}
        this.debug && console.warn('Api Gateway Endpoint not deployed:', '{{sdk.method}}');
    {{/if}}
    },
            {{/if}}
        {{/with}}
    {{/each}}
{{/each}}
};

if (window) window.{{x-apiDeploy.sdk.name}} = API;
module.exports = API;
